                <shadow type="math_number"/>
            </value>
        </block>
        <block type="executor_clear_entity">
            <value name="RADIUS">
                <shadow type="math_number"/>
            </value>
        </block>
        <block type="executor_clear_potion"/>
        <block type="executor_close_gui"/>
        <block type="executor_command">
            <value name="COMMAND">
                <shadow type="text">
                    <field name="TEXT">help</field>
                </shadow>
            </value>
        </block>
        <block type="executor_console_command">
            <value name="COMMAND">
                <shadow type="text">
                    <field name="TEXT">help</field>
                </shadow>
            </value>
        </block>
        <block type="executor_db_put">
            <value name="KEY">
                <shadow type="text"/>
            </value>
            <value name="VALUE">
                <shadow type="text"/>
            </value>
        </block>
        <block type="executor_db_save"/>
        <block type="executor_op_command">
            <value name="COMMAND">
                <shadow type="text">
                    <field name="TEXT">help</field>
                </shadow>
            </value>
        </block>
        <block type="executor_explosion">
            <value name="POWER">
