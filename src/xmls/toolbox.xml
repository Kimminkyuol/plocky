                <shadow type="text"/>
            </value>
        </block>
        <block type="executor_lightning"/>
        <block type="executor_log">
            <value name="TEXT">
                <shadow type="text"/>
            </value>
        </block>
        <block type="executor_message">
            <value name="TEXT">
                <shadow type="text"/>
            </value>
        </block>
        <block type="executor_money">
            <value name="MONEY">
